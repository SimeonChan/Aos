<%
new("nYear","int")
nYear=year()

new("szSign","string","CPAClient.Code.")
join(szSign,nYear)

//生成表格标识
new("szName","string","")
mov(szName,nYear)

new("nAuto","int")
nAuto=autonum(szSign)

small(nAuto,10,"0")
small(nAuto,100,"00")
goto("000")

flag("0")
join(szName,"00")
join(szName,nAuto)
goto("OK")

flag("00")
join(szName,"0")
join(szName,nAuto)
goto("OK")

flag("000")
join(szName,nAuto)
goto("OK")

flag("OK")
form("Code",szName)
%>